    "voip",
    "server",
    "softphone",
    "asterisk",
    "freeswitch",
    "instant-messaging",
    "ims"
  ],
  "author": "Pedro Sanders",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fonoster/routr-ctl/issues"
  },
  "homepage": "https://routr.io",
  "main": "rctl.js",
  "bin": {
    "rctl": "rctl.js"
  },
  "dependencies": {
    "command-exists": "^1.2.9",
    "routr-ui": "1.0.4",
    "shelljs": "^0.8.3"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">= 6.0.0"
  }
